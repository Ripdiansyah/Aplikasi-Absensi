	<parameter name="tanggal5" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT d.id_pegawai, p.nama,p.jabatan,p.id_jam_kerja,
sec_to_time(SUM(time_to_sec(timediff(d.jam_keluar, d.jam)))) AS total_jam_kerja
FROM detail_absen d 
join pegawai p on d.id_pegawai = p.id_pegawai
join head_absen h on d.id_absen = h.id_absen
where h.tanggal between $P{tanggal4} and $P{tanggal5}
GROUP BY d.id_pegawai]]>
	</queryString>
