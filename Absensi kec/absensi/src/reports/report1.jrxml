	<parameter name="tanggal3" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select p.id_pegawai, p.nama, p.jabatan,
count(case d.keterangan when 'Hadir' then 1 else null end) as hadir,
count(case d.keterangan when 'Sakir' then 1 else null end) as sakit,
count(case d.keterangan when 'Ijin' then 1 else null end) as ijin,
count(case d.keterangan when 'Dinas Luar' then 1 else null end) as dinas_luar,
count(case d.keterangan when 'Tanpa Keterangan' then 1 else null end) as tanpa_keterangan
from pegawai p
join detail_absen d on p.id_pegawai = d.id_pegawai 
join head_absen h on d.id_absen = h.id_absen
where h.tanggal between "$P!{tanggal1}" and "$P!{tanggal3}"
group by p.id_pegawai]]>
	</queryString>
